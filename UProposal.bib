@misc{girod22,
  title = {Thermodynamic and Electrical Transport Properties of {{UTe}}\$\_2\$ under Uniaxial Stress},
  author = {Girod, Cl{\'e}ment and Stevens, Callum R. and Huxley, Andrew and Bauer, Eric D. and Santos, Frederico B. and Thompson, Joe D. and Fernandes, Rafael M. and Zhu, Jian-Xin and Ronning, Filip and Rosa, Priscila F. S. and Thomas, Sean M.},
  year = {2022},
  month = may,
  number = {arXiv:2205.04588},
  publisher = {{arXiv}},
  url = {http://arxiv.org/abs/2205.04588},
  urldate = {2022-06-09},
  abstract = {Despite intense experimental efforts, the nature of the unconventional superconducting order parameter of UTe\$\_2\$ remains elusive. This puzzle stems from different reported numbers of superconducting transitions at ambient pressure, as well as a complex pressure-temperature phase diagram. To bring new insights into the superconducting properties of UTe\$\_2\$, we measured the heat capacity and electrical resistivity of single crystals under compressive uniaxial stress \$\textbackslash sigma\$ applied along different crystallographic directions. We find that the critical temperature \$T\_\{\textbackslash rm c\}\$ of the single observed bulk superconducting transition decreases with \$\textbackslash sigma\$ along \$[100]\$ and \$[110]\$ but increases with \$\textbackslash sigma\$ along \$[001]\$. Aside from its effect on \$T\_\{\textbackslash rm c\}\$, we notice that \$c\$-axis stress leads to a significant piezoresistivity, which we associate with the shift of the zero-pressure resistivity peak at \$T\^\textbackslash star \textbackslash approx 15\textbackslash, \textbackslash rm K\$ to lower temperatures under stress. Finally, we show that an in-plane shear stress \$\textbackslash sigma\_\{xy\}\$ does not induce any observable splitting of the superconducting transition over a stress range of \$\textbackslash sigma\_\{xy\}\textbackslash approx 0.17 \textbackslash, \textbackslash rm GPa\$. This result suggests that the superconducting order parameter of UTe\$\_2\$ may be single-component at ambient pressure.},
  archiveprefix = {arxiv},
  langid = {english},
  keywords = {Condensed Matter - Strongly Correlated Electrons,Condensed Matter - Superconductivity},
  file = {/Users/fmg12/Dropbox/papers/5fcompounds/UTe2/2205.04588.pdf}
}

@misc{hayes20,
  title = {Weyl {{Superconductivity}} in {{UTe2}}},
  author = {Hayes, Ian M. and Wei, Di S. and Metz, Tristin and Zhang, Jian and Eo, Yun Suk and Ran, Sheng and Saha, Shanta R. and Collini, John and Butch, Nicholas P. and Agterberg, Daniel F. and Kapitulnik, Aharon and Paglione, Johnpierre},
  year = {2020},
  month = feb,
  number = {arXiv:2002.02539},
  publisher = {{arXiv}},
  url = {http://arxiv.org/abs/2002.02539},
  urldate = {2022-06-09},
  abstract = {The search for a material platform for topological quantum computation has recently focused on unconventional superconductors. Such material systems, where the superconducting order parameter breaks a symmetry of the crystal point group, are capable of hosting novel phenomena, including emergent Majorana quasiparticles. Unique among unconventional superconductors is the recently discovered UTe2, where spin-triplet superconductivity emerges from a paramagnetic normal state. Although UTe2 could be considered a relative of a family of known ferromagnetic superconductors, the unique crystal structure of this material and experimentally suggested zero Curie temperature pose a great challenge to determining the symmetries, magnetism, and topology underlying the superconducting state. These emergent properties will determine the utility of UTe2 for future spintronics and quantum information applications. Here, we report observations of a non-zero polar Kerr effect and of two transitions in the specific heat upon entering the superconducting state, which together show that the superconductivity in UTe2 is characterized by an order parameter with two components that breaks time reversal symmetry. These data allow us to place firm constraints on the symmetries of the order parameter, which strongly suggest that UTe2 is a Weyl superconductor that hosts chiral Fermi arc surface states.},
  archiveprefix = {arxiv},
  langid = {english},
  keywords = {Condensed Matter - Strongly Correlated Electrons,Condensed Matter - Superconductivity},
  file = {/Users/fmg12/Dropbox/papers/5fcompounds/UTe2/hayes20.pdf}
}

