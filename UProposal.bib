@article{bauer01a,
  title = {Coexistence of Superconductivity and Ferromagnetism in Polycrystalline {{UGe}} {\textsubscript{2}}},
  author = {Bauer, E D and Dickey, R P and Zapf, V S and Maple, M B},
  year = {2001},
