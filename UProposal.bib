@misc{weinberger23,
  title = {Quantum Interference between Quasi-{{2D Fermi}} Surface Sheets in {{UTe2}}},
  author = {Weinberger, T. I. and Wu, Z. and Graf, D. E. and Skourski, Y. and Cabala, A. and Pospisil, J. and Prokleska, J. and Haidamak, T. and Bastien, G. and Sechovsky, V. and Lonzarich, G. G. and Valiska, M. and Grosche, F. M. and Eaton, A. G.},
